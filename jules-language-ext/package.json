{
    "name": "jules-language-ext",
    "displayName": "Jules Language",
    "description": "Language support for the Jules programming language",
    "version": "0.1.0",
    "publisher": "jules",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "type": "git",
        "url": ""
    },
    "contributes": {
        "languages": [
            {
                "id": "jules",
                "aliases": [
                    "Jules",
                    "jules"
                ],
                "extensions": [
                    ".jules"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "jules",
                "scopeName": "source.jules",
                "path": "./syntaxes/jules.tmLanguage.json"
            }
        ]
    }
} 